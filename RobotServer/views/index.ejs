<!DOCTYPE html>
<html>
<head>
	<title>NodeJSRobot</title>
    <link href="style.css" rel="stylesheet">
</head>
<body>
	<nav>
		<a href="#">Robot Controller</a>
		<a href="#">About</a>
		<a href="#">Contact</a>
	</nav>
	<div class="container">
		<div class="frame">
			<div id="unity-container" class="unity-desktop">
				<canvas id="unity-canvas" width=100 height=100></canvas>
				<div id="unity-loading-bar">
				  <div id="unity-logo"></div>
				  <div id="unity-progress-bar-empty">
					<div id="unity-progress-bar-full"></div>
				  </div>
				</div>
				<div id="unity-warning"> </div>
				<div id="unity-footer">
				  <div id="unity-webgl-logo"></div>
				  <div id="unity-fullscreen-button"></div>
				</div>
			  </div>
		</div>
	</div>
	<div class="buttons">
		<button class="arrow-button" id="left-button">&#8592;</button> <!-- Left arrow -->
		<button class="arrow-button" id="up-button">&#8593;</button> <!-- Up arrow -->
		<button class="arrow-button" id="right-button">&#8594;</button> <!-- Right arrow -->
		<button class="arrow-button" id="down-button">&#8595;</button> <!-- Down arrow -->
	</div>
    <script src="script.js"></script>
</body>
</html>